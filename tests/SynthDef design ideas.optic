//Synthfunc syntax

//Squiggly Arrow
*sinSynth <f=440 mf=220 mf2=100.5 g=1> dur a ch =
	msin ~> filter( ~ 1200 * env ) * env * a ~> fout( ~ ) ~> out( ch ~ ) ~> drawScope( ~ )
	where
		env = adr( [ 1 0.5 ] [ 0.1 0.2 ] -3 g )
		mod = sin( f ) * sin( 0.1 )  ~> range( ~ -100 300 )
		mod2 = saw( f ) * tri( 0.1 ) ~> range( ~ -100 300 )
		f2 = f*mod( [ mf mf2 ] ) * fin()
		sa = mod2( [ mf2 mf ] )
		msin = sin( f2 sa )

//vs

//Composition operator
*sinSynth <f=440 mf=220 mf2=100.5 g=1> dur a ch =
	sin( f2 sa ) >>> filter( 1200 * env ) * env * a >>> fout >>> out( ch ) >>> drawScope
	where
		env = adr( [ 1 0.5*dur ] [ 0.1 0.2 ] -3 g )
		mod = sin( f ) * sin( 0.1 )  >>> range( -100 300 )
		mod2 = saw( f ) * tri( 0.1 ) >>> range( -100 300 )
		f2 = f*mod( [ mf mf2 ] ) * fin()
		sa = mod2( [ mf2 mf ] )

*sinSynth <f=440 mf=220 mf2=100.5 g=1> dur a ch = ds
	where
		env = adr( [ 1 0.5 ] [ 0.1 0.2 ] -3 g )
		mod = range( sin( f ) * sin( 0.1 )  100 300 )
		mod2 = range( saw( f ) * tri( 0.1 ) -100 300 )
		f2 = f*mod( [ mf mf2 ] ) * fin()
		sa = mod2( [ mf2 mf ] )
		msin = sin( f2 sa )
		filt = filter(msin 1200 * env) * env * a
		feed = fout(filt)
		o = out(feed)
		ds = drawScope(o)


